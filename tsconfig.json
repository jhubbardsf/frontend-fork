{
  "compilerOptions": {
    "skipDefaultLibCheck": true,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "sourceMap": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
    }
  },
  "include": [
    ".env.local",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/utils/script.js",
    "scripts/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}